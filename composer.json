{
    "name": "creode/laravel-nova-events-nested-categories",
    "description": "A Laravel Nova package that provides an Events resource that supports nested categories",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Creode\\LaravelNovaEventsNestedCategories\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creode\\LaravelNovaEventsNestedCategories\\LaravelNovaEventsNestedCategoriesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
